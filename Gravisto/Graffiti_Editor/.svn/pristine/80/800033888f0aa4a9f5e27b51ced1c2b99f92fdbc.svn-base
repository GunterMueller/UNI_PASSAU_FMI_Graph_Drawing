package crossing;
import org.graffiti.plugin.GenericPluginAdapter;
import org.graffiti.plugin.algorithm.Algorithm;


/**
 * The Plugin class for FindIntersections
 * 
 * @author Daniel Hanisch
 */
public class FindIntersectionsPlugin extends GenericPluginAdapter {
    /**
     * Constructor
     */
    public FindIntersectionsPlugin() {
        this.algorithms = new Algorithm[1];
        this.algorithms[0] = new FindIntersections();
    }
}