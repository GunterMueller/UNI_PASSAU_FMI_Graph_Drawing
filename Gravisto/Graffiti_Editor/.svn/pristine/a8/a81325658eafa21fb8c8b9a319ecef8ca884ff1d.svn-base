package org.graffiti.editor;

import javax.swing.UIManager;

/**
 * @author donig
 * @version $Revision$ $Date$
 */
enum LookAndFeel {

    JGOODIES_WINDOWS("com.jgoodies.looks.windows.WindowsLookAndFeel"),

    SUN_WINDOWS("com.sun.java.swing.plaf.windows.WindowsLookAndFeel"),

    SUN_CROSS_PLATFORM(UIManager.getCrossPlatformLookAndFeelClassName());

    private String className;

    LookAndFeel(String className) {
        this.className = className;
    }

    String getClassName() {
        return className;
    }

}

