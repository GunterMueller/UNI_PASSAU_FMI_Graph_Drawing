package quoggles.exceptions;

/**
 * 
 */
public class BoxCreationFailedException extends QuogglesException {

    /**
     * 
     */
    public BoxCreationFailedException() {
        super();
    }

    /**
     * @param message
     */
    public BoxCreationFailedException(String message) {
        super(message);
    }

    /**
     * @param ex
     */
    public BoxCreationFailedException(Throwable ex) {
        super(ex);
    }
}
