package org.visnacom.model;

/**
 * @author F. Pfeiffer
 *
 * This exception is thrown if someone tries to make a cluster out of nodes with
 * no common parent.
 */
public class NoCommonParentException extends RuntimeException{

	/**
	 * Constructor.
	 * 
	 * @param err
	 *            Error message.
	 */
	public NoCommonParentException(String err) {
		super(err);
	}
	
}
