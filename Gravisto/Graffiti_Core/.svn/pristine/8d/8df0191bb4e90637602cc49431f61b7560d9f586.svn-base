package org.graffiti.plugins.algorithms.chebyshev.benchmark;

import org.graffiti.plugins.algorithms.chebyshev.MCMAbstractAlgorithm;
import org.graffiti.plugins.algorithms.chebyshev.MCMCrossMinAlgorithm;
import org.graffiti.plugins.algorithms.sugiyama.layout.LayoutAlgorithm;

/**
 * @author Andreas Glei&szlig;ner
 * @version $Revision$ $Date$
 */
public class IdleLayout extends MCMAbstractAlgorithm implements LayoutAlgorithm {
    private static final String NAME = MCMCrossMinAlgorithm
            .getString("idleLayout.name");

    /**
     * {@inheritDoc}
     */
    @Override
    public void execute() {
    }

    /**
     * {@inheritDoc}
     */
    @Override
    public String getName() {
        return NAME;
    }

    /**
     * {@inheritDoc}
     */
    @Override
    public boolean supportsArbitraryXPos() {
        return true;
    }
}

