package org.graffiti.plugins.editcomponents.yagi;

import javax.swing.JComponent;

import org.graffiti.plugin.editcomponent.ValueEditComponent;

/**
 * Value edit components implementing {@code SelfLabellingComponent} potentially
 * maintain a {@code JComponent} that contains a label for itself.
 * 
 * @author Andreas Glei&szlig;ner
 * @see ValueEditComponent
 * @see JComponent
 */
public interface SelfLabelingComponent {
    /**
     * Returns if this maintains a {@code JComponent} that contains a label for
     * itself.
     * 
     * @return {@code true} if this maintains a {@code JComponent} that contains
     *         a label for itself.
     * @see ValueEditComponent
     * @see JComponent
     */
    public boolean isSelfLabeling();
}
