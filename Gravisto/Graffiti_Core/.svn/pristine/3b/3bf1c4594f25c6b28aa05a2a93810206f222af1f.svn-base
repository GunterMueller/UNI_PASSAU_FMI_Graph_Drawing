package org.graffiti.plugins.ios.importers.nexus;

/**
 * Constants used throughout parsing of NEXUS or Newick files.
 */
public interface NexusParsingConstants {
    /**
     * Error message when the file given cannot be parsed.
     */
    static final String FILE_PARSE_ERROR = "Input file incomplete.";

    /**
     * Error message given if the parsing of a Newick tree fails.
     */
    static final String TREE_PARSE_ERROR = "Cannot parse tree.";

    /**
     * The content of the first line of NEXUS-files.
     */
    static final String NEXUS_FILE_DESCRIPTOR = "#NEXUS";

    /** The command name of a tree in a NEXUS file. */
    static final String COMMAND_TREE = "tree";

    /** The command name of a name translation in a NEXUS file. */
    static final String COMMAND_TRANSLATION = "translate";

    /** The name of a tree-block in a NEXUS file. */
    static final String BLOCK_NAME_TREES = "trees";

    /** The command that signals the beginning of a block in a NEXUS file. */
    static final String BLOCK_BEGIN = "begin";

    /** The command that signals the end of a block in a NEXUS file. */
    static final String BLOCK_END = "end";
}
