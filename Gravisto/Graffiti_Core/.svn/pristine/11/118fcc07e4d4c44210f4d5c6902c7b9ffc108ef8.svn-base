package org.graffiti.plugins.ios.importers.dmf;

import org.graffiti.plugin.GenericPluginAdapter;

/**
 * This plugin provides the functionality for reading dot files.
 * 
 * @author keilhaue
 */
public class DMFReaderPlugin extends GenericPluginAdapter {

    /**
     * Constructs a new <code>DotReaderPlugin</code>.
     */
    public DMFReaderPlugin() {
        super();
        this.inputSerializers = new DMFReader[1];
        this.inputSerializers[0] = new DMFReader();
    }
}

