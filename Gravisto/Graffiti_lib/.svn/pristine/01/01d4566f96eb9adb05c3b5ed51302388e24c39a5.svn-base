package quoggles.stdboxes.sortby;

import quoggles.boxes.IBox;
import quoggles.icons.AbstractBoxIcon;

/**
 * Icon for <code>SortBy_Box</code>
 */
public class SortBy_Icon extends AbstractBoxIcon {

    public SortBy_Icon() {
        label.setText("sort by");
        adjustSize();
    }

    /**
     * @see quoggles.icons.IBoxIcon#getNewBoxInstance()
     */
    public IBox getNewBoxInstance() {
