package org.graffiti.plugin.io;

/**
 * Defines a generic serializer which provides a set of extensions.
 */
public interface Serializer {

    /**
     * The file extensions the serializer can read or write.
     * 
     * @return DOCUMENT ME!
     */
    public String[] getExtensions();
}

