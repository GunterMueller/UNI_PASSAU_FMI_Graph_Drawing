package org.graffiti.core;

/**
 * Guarantees a deep copy.
 * 
 * @version $Revision$
 */
public interface DeepCopy {
    /**
     * Returns a deep copy of this object.
     * 
     * @return A deep copy of this object.
     */
    public Object copy();
}

