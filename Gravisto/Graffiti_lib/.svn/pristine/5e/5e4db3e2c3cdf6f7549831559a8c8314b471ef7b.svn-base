package org.graffiti.plugins.inspectors.defaults;

/**
 * Represents the tab of the instpector, which edits the properties of a node.
 * 
 * @version $Revision$
 */
public class NodeTab extends AbstractTab {

    /**
     * 
     */
    private static final long serialVersionUID = 5483407868630099165L;

    /**
     * Constructs a <code>NodeTab</code> and sets the title.
     */
    public NodeTab() {
        super();
        this.title = "Node";
    }
}

