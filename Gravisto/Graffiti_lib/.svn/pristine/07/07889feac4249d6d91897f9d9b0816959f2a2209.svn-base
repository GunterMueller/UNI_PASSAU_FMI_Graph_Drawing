package org.graffiti.plugins.algorithms.stackqueuelayout;

import org.graffiti.session.Session;
import org.graffiti.session.SessionListener;

/**
 * @author Andreas Glei&szlig;ner
 * @version $Revision$ $Date$
 */
class StackQueueSessionListener implements SessionListener {
    public StackQueueSessionListener() {
        //
    }

    /*
     * @see
     * org.graffiti.session.SessionListener#sessionChanged(org.graffiti.session
     * .Session)
     */
    @Override
    public void sessionChanged(Session s) {
        System.out.println("sessionChanged: " + s);
    }

    /*
     * @see
     * org.graffiti.session.SessionListener#sessionDataChanged(org.graffiti.
     * session.Session)
     */
    @Override
    public void sessionDataChanged(Session s) {
        System.out.println("sessionDataChanged: " + s);
    }

}

