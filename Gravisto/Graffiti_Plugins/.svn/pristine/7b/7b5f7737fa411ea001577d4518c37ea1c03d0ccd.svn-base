package quoggles.auxiliary;

/**
 * A class that encapsulates exactly three objects.
 */
public class Triple {
    
    private Object fst;
    
    private Object snd;
    
    private Object thd;
    
    
    public Triple(Object f, Object s, Object t) {
        fst = f;
        snd = s;
        thd = t;
    }
    
    
    /**
     * @return
     */
    public Object getFst() {
        return fst;
    }

    /**
     * @param fst
     */
    public void setFst(Object fst) {
        this.fst = fst;
    }

    /**
     * @return
     */
    public Object getSnd() {
        return snd;
    }

    /**
     * @param snd
     */
    public void setSnd(Object snd) {
        this.snd = snd;
    }

    /**
     * @return
     */
    public Object getThd() {
        return thd;
    }

    /**
     * @param thd
     */
    public void setThd(Object thd) {
        this.thd = thd;
    }

}