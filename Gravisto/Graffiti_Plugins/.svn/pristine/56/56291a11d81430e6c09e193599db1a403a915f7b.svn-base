package org.graffiti.plugins.guis.graphediting;

import org.graffiti.core.Bundle;

/**
 * Identifies the resource bundle used by this package.
 * 
 * @see Bundle
 */
public final class GraphEditingBundle {
    private static final Bundle bundle = Bundle
            .getBundle(GraphEditingBundle.class);

    private GraphEditingBundle() {
    }

    /**
     * See {@link Bundle#getString(String)}.
     */
    public static String getString(String key) {
        return bundle.getString(key);
    }
}
