package org.graffiti.plugins.algorithms.GeoThickness;

/**
 * @author ma
 * 
 *         TODO To change the template for this generated type comment go to
 *         Window - Preferences - Java - Code Style - Code Templates
 */
public class LocalEdge {

    private double leftNodeX, leftNodeY, rightNodeX, rightNodeY;

    private Integer id;

    public LocalEdge(Integer id, double leftX, double leftY, double rightX,
            double rightY) {
        this.id = id;
        this.leftNodeX = leftX;
        this.leftNodeY = leftY;
        this.rightNodeX = rightX;
        this.rightNodeY = rightY;
    }

    public Integer getID() {
        return this.id;
    }

    public double getLeftX() {
        return this.leftNodeX;
    }

    public double getLeftY() {
        return this.leftNodeY;
    }

    public double getRightX() {
        return this.rightNodeX;
    }

    public double getRightY() {
        return this.rightNodeY;
    }

}
