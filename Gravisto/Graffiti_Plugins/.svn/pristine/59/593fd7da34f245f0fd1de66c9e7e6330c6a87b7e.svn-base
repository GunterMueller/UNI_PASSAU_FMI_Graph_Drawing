package org.graffiti.plugins.ios.exporters.pstricks;

import org.graffiti.plugin.GenericPluginAdapter;
import org.graffiti.plugin.io.OutputSerializer;

public class PSTricksSerializerPlugin extends GenericPluginAdapter {
    /**
     * Creates a new PSTricksSerializerPlugin object.
     */
    public PSTricksSerializerPlugin() {
        this.outputSerializers = new OutputSerializer[1];
        this.outputSerializers[0] = new PSTricksSerializer();
    }
}
