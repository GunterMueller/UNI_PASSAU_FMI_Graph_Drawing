GRAPHLET_BASE_DIR=../../..
SUBDIRS=

include $(GRAPHLET_BASE_DIR)/lib/graphlet/config/common

EXEC_FILES_TO_INSTALL= binary-installer

FILES_TO_INSTALL= $(EXEC_FILES_TO_INSTALL) \
	binary-installer \
	cc-mode.el \
	COPYRIGHT ACKNOWLEDGEMENTS

all:	COPYRIGHT
	$(CHMOD_EXEC) $(EXEC_FILES_TO_INSTALL)

install.local::	$(FILES_TO_INSTALL)
	$(INSTALL_MAKE_DIR) $(INSTALL_ETC_DIR)
	$(INSTALL_FILE) $(FILES_TO_INSTALL) $(INSTALL_ETC_DIR)
	$(CHMOD_EXEC) $(INSTALL_ETC_DIR)/$(EXEC_FILES_TO_INSTALL)

COPYRIGHT:	$(GRAPHLET_BASE_DIR)/COPYRIGHT
	$(CP) $< $@

ACKNOWLEDGEMENTS:	$(GRAPHLET_BASE_DIR)/ACKNOWLEDGEMENTS
	$(CP) $< $@

